<h1>Kalendarz sesyjny</h1>

<% @lists.each do |list| %>

<h2><%= link_to "#{list.name}", list_url(list) %></h2>
<p><%= list.description %> - <%= link_to "Edytuj", edit_list_url(list) %> | <%= link_to "Usun", list_url(list), :method => :delete %></p>

<% end %>

<hr />

<%= link_to "Dodaj nowy kalendarz", new_list_url %>